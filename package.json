{
  "name": "evaluaciones-taq",
  "version": "1.0.0",
  "description": "Sistema completo de gestión y evaluación de locales comerciales",
  "main": "index.js",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:admin && npm run install:evaluacion",
    "install:backend": "cd backend && npm install",
    "install:admin": "cd frontend/administrador && npm install",
    "install:evaluacion": "cd frontend/evaluacion && npm install",
    "start:backend": "cd backend && npm start",
    "start:admin": "cd frontend/administrador && npm start",
    "start:evaluacion": "cd frontend/evaluacion && npm start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:admin\" \"npm run start:evaluacion\"",
    "build:admin": "cd frontend/administrador && npm run build",
    "build:evaluacion": "cd frontend/evaluacion && npm run build",
    "build:all": "npm run build:admin && npm run build:evaluacion",
    "verify": "node verificar-instalacion.js",
    "prepare:production": "bash install-production.sh",
    "prepare:production:win": "install-production.bat"
  },
  "keywords": [
    "evaluaciones",
    "locales",
    "react",
    "nodejs",
    "mysql",
    "dashboard"
  ],
  "author": "Guillermo Angel",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuillermoAngel27/evaluaciones_TAQ.git"
  },
  "bugs": {
    "url": "https://github.com/GuillermoAngel27/evaluaciones_TAQ/issues"
  },
  "homepage": "https://github.com/GuillermoAngel27/evaluaciones_TAQ#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.7.1",
    "reactstrap": "^9.2.3"
  }
}
